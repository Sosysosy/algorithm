<body>
    <script>
       var printNumbers = function(n) {
    var sum = function (n) {             //递归
            if (n == 1) {
                return 10;
            }
            return sum(--n) * 10;
        };
        var num=sum(n)-1;
        var arr=[];
        for(var i=1;i<=num;i++){
              arr.push(i);
        }
        return arr;
};
          console.log(printNumbers(1));
    </script>
</body>

